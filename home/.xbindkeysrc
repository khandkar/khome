"xbindkeys_show"
  control+shift + q

# Increase volume
"vol_inc"
   XF86AudioRaiseVolume
# Decrease volume
"vol_dec"
   XF86AudioLowerVolume
# Mute
"vol_mute"
   XF86AudioMute

"backlight_adjust +5"
    XF86MonBrightnessUp
"backlight_adjust -5"
    XF86MonBrightnessDown

# mate-screensaver equivalent: "mate-screensaver-command --lock"
"xscreensaver-command -activate"
   XF86ScreenSaver
"xscreensaver-command -activate"
   control + alt + l
#"lock"
#   XF86ScreenSaver
#"lock"
#   control + alt + l

# ----------
# MPD client
# ----------
# If keyboard has media keys, use them:
"mpc toggle"
    XF86AudioPlay
"mpc stop"
    XF86AudioStop
"mpc prev"
    XF86AudioPrev
"mpc next"
    XF86AudioNext
# Otherwise use combinations:
# (Mod4 is Windows key)
"mpc toggle"
    Mod4+Down
"mpc stop"
    Mod4+Up
"mpc prev"
    Mod4+Left
"mpc next"
    Mod4+Right

