#! /bin/sh

# www browser
ln -sf ~/bin/www_dpi_high ~/bin/www

# qutebrowser specifically
ln -sf ~/.config/qutebrowser/config_for_dpi_high.py ~/.config/qutebrowser/config_for_dpi.py

# dunst
ln -sf ~/bin/dunst_dpi_high ~/bin/dunst_dpi

# st
ln -sf /usr/local/bin/st_dpi_high /usr/local/bin/st

# dwm
ln -sf /usr/local/bin/dwm_dpi_high /usr/local/bin/dwm

ln -sf ~/lib/login_variables_dpi_high.sh ~/lib/login_variables_dpi.sh
