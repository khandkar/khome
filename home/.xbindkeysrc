# Quick access to in-terminal apps
"st -e htop"
    Mod4+h
"st -e ncmpcpp"
    Mod4+n
"st -e ranger"
    Mod4+r

# Transparency adjust
"compton-trans 40"
    Mod4 + 4
"compton-trans 45"
    Mod4 + Shift + 4
"compton-trans 50"
    Mod4 + 5
"compton-trans 55"
    Mod4 + Shift + 5
"compton-trans 60"
    Mod4 + 6
"compton-trans 65"
    Mod4 + Shift + 6
"compton-trans 70"
    Mod4 + 7
"compton-trans 75"
    Mod4 + Shift + 7
"compton-trans 80"
    Mod4 + 8
"compton-trans 85"
    Mod4 + Shift + 8
"compton-trans 90"
    Mod4 + 9
"compton-trans 95"
    Mod4 + Shift + 9
"compton-trans 100"
    Mod4 + 0

