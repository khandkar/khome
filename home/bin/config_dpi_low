#! /bin/sh

# www browser
ln -sf ~/bin/www_dpi_low ~/bin/www

# qutebrowser
ln -sf ~/.config/qutebrowser/config_for_dpi_low.py ~/.config/qutebrowser/config_for_dpi.py

# dunst
ln -sf ~/bin/dunst_dpi_low ~/bin/dunst_dpi

# st
ln -sf /usr/local/bin/st_dpi_norm /usr/local/bin/st

# dwm
ln -sf /usr/local/bin/dwm_dpi_low /usr/local/bin/dwm

ln -sf ~/lib/login_variables_dpi_norm.sh ~/lib/login_variables_dpi.sh
